{
  "name": "graphql-schema-query",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "keywords": [
    "graphql",
    "schema",
    "json"
  ],
  "author": "Pierre-Louis Mercereau",
  "homepage": "",
  "bugs": "https://github.com/plmercereau/graphql-schema-query/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/plmercereau/graphql-schema-query.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist",
    "umd",
    "README.md"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "node": "./dist/index.cjs.js",
        "default": "./dist/index.esm.js"
      },
      "require": "./dist/index.cjs.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite build --config ../../config/vite.lib.dev.config.js",
    "build": "run-p build:lib build:generate",
    "build:lib": "vite build",
    "build:generate": "vite build --config ./vite.config.generate.js",
    "generate": "pnpm run build:generate && node dist/generate.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "e2e": "start-test e2e:backend http-get://localhost:9695 ci:test",
    "ci:test": "vitest run --config vite.config.e2e.js",
    "e2e:backend": "nhost dev --no-browser",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "json-to-graphql-query": "^2.2.4"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.3",
    "@graphql-codegen/typescript": "^2.7.1",
    "@types/node": "^18.11.18",
    "graphql": "^16.6.0",
    "jsdom": "^21.0.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.26.3"
  },
  "packageManager": "pnpm@7.17.0",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7.17.0"
  }
}